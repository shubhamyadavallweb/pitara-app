{
  "appId": "com.pitara.app",
  "appName": "Pitara",
  "webDir": "dist",
  "bundledWebRuntime": false,
  "server": {
    "androidScheme": "https",
    "cleartext": true
  },
  "plugins": {
    "CapacitorHttp": {
      "enabled": true
    },
    "SplashScreen": {
      "launchAutoHide": false
    },
    "App": {
      "url": "pitara://auth/callback"
    }
  }
} 